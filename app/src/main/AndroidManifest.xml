<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testvoronkov">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestVoronkov">
        <activity
            android:name=".ui.theme.TestOpen"
            android:exported="true"
            android:label="@string/title_activity_test_open"
            android:theme="@style/Theme.TestVoronkov.NoActionBar" />
        <activity
            android:name=".EventOpen"
            android:exported="true"
            android:label="@string/title_activity_event_open"
            android:theme="@style/Theme.TestVoronkov.NoActionBar" />
        <activity
            android:name=".Belakov"
            android:exported="false"
            android:label="@string/title_activity_belakov"
            android:theme="@style/Theme.TestVoronkov.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.TestVoronkov.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>